	<gazebo reference="base_laser_link">
		<sensor name="base_laser" type="ray">
			<pose>0 0 0 0 0 0</pose>
			<update_rate>10</update_rate>
			<visualize>true</visualize>
			<ray>
				<scan>
					<horizontal>
						<!-- 818 (270/0.33) steps in 270deg fov -->
						<samples>666.5185185185185</samples>
						<resolution>1</resolution>
						<!-- not the sensor resolution; just 1 -->
						<min_angle>-1.9198621771937625</min_angle>
						<max_angle>1.9198621771937625</max_angle>
					</horizontal>
				</scan>
				<range>
					<min>0.05</min>
					<max>25.0</max>
					<resolution>0.001</resolution>
				</range>
				<noise>
					<type>gaussian</type>
					<mean>0.0</mean>
					<stddev>0.06</stddev>
				</noise>
			</ray>
			<plugin filename="libgazebo_ros_ray_sensor.so" name="bytes_laserscan">
				<ros>
					<remapping>~/out:=scan_raw</remapping>
				</ros>
				<output_type>sensor_msgs/LaserScan</output_type>
				<frame_name>base_laser_link</frame_name>
			</plugin>
		</sensor>
		<material>Gazebo/DarkGrey</material>
	</gazebo>
